source $BYOBU_PREFIX/share/byobu/keybindings/common
escape ^Jj
register r "^j:source $HOME/.byobu/profile^M"		#     | Goes with F5 definition
register t "^jA^aa^k^h"					#     | Goes with the F8 definition
register s "^j[g G$>^h"					# Goes with ^a~ definition
register d "^j:source /usr/share/byobu/keybindings/screen-escape-keys^M"
register r "^j:source $HOME/.byobu/profile^M"
register e "^j:source /usr/share/byobu/keybindings/f-keys^M"
